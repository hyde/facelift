if icons
   @font-face
      font-family 'icon'
      src url('http://localhost:8000/moot/ui/dist/font/moot-regular-webfont.eot')
      src url('http://localhost:8000/moot/ui/dist/font/moot-regular-webfont.eot?#iefix') format('embedded-opentype'), url('http://localhost:8000/moot/ui/dist/font/moot-regular-webfont.woff') format('woff'), url('http://localhost:8000/moot/ui/dist/font/moot-regular-webfont.ttf') format('truetype'), url('http://localhost:8000/moot/ui/dist/font/moot-regular-webfont.svg#MootRegular') format('svg')
      font-weight normal
      font-style normal

icon(char)
   font-family icon
   content "'%s'" % char

